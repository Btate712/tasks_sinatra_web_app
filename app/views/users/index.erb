<h1>System Users:</h1>
<ul class="align-left">
  <% @users.each do |user| %>
    <li><a href="/users/<%= user.slug %>">
      <%= user.name %></a> (<%= user.username %>)
    </li>
  <% end %>
</ul>
<form class="align-left" method="GET" action="/tasks/users/<%= current_user.slug %>">
  <input type="submit" value="Back to Task List">
</form>
